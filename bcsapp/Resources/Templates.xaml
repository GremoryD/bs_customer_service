<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:bcsapp.Views"
                    xmlns:vm="clr-namespace:bcsapp.ViewModels"
                    xmlns:local="clr-namespace:bcsapp.Resources">
    <DataTemplate DataType="{x:Type vm:LoginViewModel}">
        <v:LoginView></v:LoginView>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ClientViewModel}">
        <v:ClientView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UserViewModel}">
        <v:UsersView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SettingsViewModel}">
        <v:SettingsView />
    </DataTemplate> 
    <DataTemplate DataType="{x:Type vm:CustomersViewModel}">
        <v:CustomersView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AplicationViewModel}">
        <v:AplicationView/>
    </DataTemplate>
</ResourceDictionary>