<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:bcsapp.Views"
                    xmlns:vm="clr-namespace:bcsapp.ViewModels"
                    xmlns:local="clr-namespace:bcsapp.Resources">
    <DataTemplate DataType="{x:Type vm:LoginViewModel}">
        <v:LoginView></v:LoginView>
    </DataTemplate> 
    <DataTemplate DataType="{x:Type vm:AplicationViewModel}">
        <v:AplicationView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddUserViewModel}">
        <v:AddUserView/>
    </DataTemplate> 
</ResourceDictionary>